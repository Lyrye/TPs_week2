<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="helloWorldBean" class="org.imt.nordeurope.j2ee.tps_week2.nickler.TpSpringBoot.HelloWorldBean"/>

    <bean id="helloWorldBeanInjection" class="org.imt.nordeurope.j2ee.tps_week2.nickler.TpSpringBoot.HelloWorldBeanInjection">
        <property name="helloWorldBean" ref="helloWorldBean"/>
    </bean>

    <bean id="temperatureServiceBeanImplSingleton" class="org.imt.nordeurope.j2ee.tps_week2.nickler.TpSpringBoot.Bean.TemperatureServiceBeanImpl" scope="singleton"/>



</beans>